{
  "SwaggerSettings": {
    "Title": "FoldingCash API",
    "Version": "v1",
    "SwaggerUrl": "",
    "JsonUrl": "/swagger/v1/swagger.json"
  },
  "DatabaseSettings": {
    "ConnectionString": "Server=.;Database=FoldingCash;Integrated Security=true;ConnectRetryCount=1;",
    "DatabaseCacheSettings": {
      "Days": 0,
      "Hours": 1,
      "Minutes": 0
    }
  },
  "DataStoreSettings": {
    "Type": "Unc",
    "UploadDirectory": "\\uploaddirectory",
    "DataStoreCacheSettings": {
      "Directory": "\\datastorecache",
      "Days": 7,
      "Hours": 0,
      "Minutes": 0
    }
  },
  "DownloadSettings": {
    "AcceptAnySslCert": false,
    "DownloadTimeout": 0,
    "DownloadUri": "https://apps.foldingathome.org/daily_user_summary.txt.bz2",
    "MinimumWaitTimeInHours": 1,
    "DownloadDirectory": "\\downloaddirectory"
  },
  "EmailSettings": {
    "Enabled": true,
    "Host": "smtp.gmail.com",
    "Port": 587,
    "Address": "From@Address.tld",
    "DisplayName": "Display Name",
    "Password": "Password",
    "Receivers": [
      "listOfReceivers@domain.tld",
      "more@domain.tld"
    ]
  },
  "FilterSettings": {
    "EnableNoPaymentAddressUsersFilter": true,
    "EnableWhitespaceNameUsersFilter": true,
    "EnableZeroPointUsersFilter": true,
    "EnableGoogleUsersFilter": true
  },
  "DateTimeSettings": {
    "Formats": []
  },
  "Logging": {
    "EventLog": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  }
}